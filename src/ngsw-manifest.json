{
  "external": {
    "urls": []
  },
  "static.ignore": [
    "\\.js\\.map$"
  ],
  "static.versioned": [
    "\\.[0-9a-z]{20}\\."
  ],
  "dynamic": {
    "group": [
      {
        "name": "online",
        "urls": {
          "http://localhost:4200/api": {
            "match": "prefix"
          }
        },
        "cache": {
          "optimizeFor": "freshness",
          "networkTimeoutMs": 200,
          "maxAgeMs": 1000,
          "maxEntries": 2,
          "strategy": "fifo"
        }
      },
      {
        "name": "offline",
        "urls": {
          "http://localhost:4200/api": {
            "match": "prefix"
          }
        },
        "cache": {
          "optimizeFor": "performance",
          "networkTimeoutMs": 200,
          "maxAgeMs": 36000,
          "maxEntries": 2,
          "strategy": "lru"
        }
      },
      {
        "name": "something",
        "urls": {
          "http://localhost:4200/api": {
            "match": "prefix"
          }
        },
        "cache": {
          "optimizeFor": "performance",
          "networkTimeoutMs": 200,
          "maxAgeMs": 36000,
          "maxEntries": 2,
          "strategy": "lfu"
        }
      }
    ]
  },
  "routing": {
    "index": "/index.html",
    "routes": {
      "/": {
        "prefix": false
      },
      "/home": {
        "prefix": true
      },
      "/heroes": {
        "prefix": false
      },
      "/villains": {
        "prefix": false
      }
    }
  },
  "push": {
    "showNotifications": true,
    "backgroundOnly": true
  }
}
